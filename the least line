#include<stdlib.h>
#include<stdio.h>
#include<time.h>
int main()
{
    srand(time(NULL));
    int n[5][5],check[26]={0};
    int i=0,j=0,temp,k;
    int sum=0;
    for(i=0;i<5;i++)
    {
        for(j=0;j<5;)
        {
            n[i][j]=rand()%25+1;
            if(check[n[i][j]] == 0)
            {
                check[n[i][j]] = 1;
                printf("%d\t",n[i][j]);
                j++;
            }
        }
        printf("\n");
    }
    k=0,i=0,j=0;
    while(k<8)
    {
        if(i==4 && j==4)
            break;
        if(i==4 && j<5)
        {
          printf("right %d\n",n[i][j+1]);
          sum+=n[i][j+1];
          j++;
        }
        else if(j==4 && i<5)
        {
            printf("down %d\n",n[i+1][j]);
            sum+=n[i+1][j];
            i++;
        }
        else if (i<5 && j<5)
        {
            if(n[i][j+1] > n[i+1][j])
            {
            printf("right %d\n",n[i][j+1]);
            sum+=n[i][j+1];
            j++;
            }
            if(n[i][j+1] < n[i+1][j])
            {
            printf("down %d\n",n[i+1][j]);
            sum+=n[i+1][j];
            i++;
            }
        }
        k++;
    }
    printf("Good job!\n");
    printf("steps:%d",sum);
}
