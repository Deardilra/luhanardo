/*be a hacker 2019/10/21*/
#include <stdio.h>
#include <stdlib.h>

#include<stdio.h>
#include<stdlib.h>
int main()
{
    /*Open file*/
    FILE*input;
    input=fopen("password.txt","r");

    /*Variables*/
    int number;
    int com=1000000;
    int num[7];
    int status;

    /*Compute with EOF*/

    status=fscanf(input,"%d",&number);
    while(status != EOF)
    {
        printf("The number:%d\n",number);
        printf("The changed number:");
        com=1000000;
        for(int i=0;i<7;i++)
        {
            num[i]=((number/com)+3)%10;
            number%=com;
            com=com/10;
            printf("%d",num[i]);
        }
        status=fscanf(input,"%d",&number);
    }
    fclose(input);
    system("pause");
    return 0;
}

