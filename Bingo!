#include<stdio.h>
#include<stdlib.h>
#include<time.h>
typedef int bool;
#define true 1
#define false 0
int main()
{
    int arr1[5][5],arr2[5][5];
    int check1[25]={0},check2[25]={0},check[25]={0};
    int i,j;
    int a=0,b=0;
    int player,computer;
    srand(time(NULL));
    for(i=0;i<5;i++)
    {
        for(j=0;j<5;j++)
        {
            while(1)
            {
                arr1[i][j]=rand()%25+1;
                if(check1[arr1[i][j]-1]==0)
                {
                    check1[arr1[i][j]-1]=1;
                    break;
                }
            }
        }
        printf("\n");
    }
    printf("\n");
    for(i=0;i<5;i++)
    {
        for(j=0;j<5;j++)
        {
            while(1)
            {
                arr2[i][j]=rand()%25+1;
                if(check2[arr2[i][j]-1]==0)
                {
                    check2[arr2[i][j]-1]=1;
                    break;
                }
            }
        }
        printf("\n");
    }
    while(1)
    {
        a=0,b=0;
        for(i=0;i<5;i++)
        {
            for(j=0;j<5;j++)
            {
                printf("%4d",arr1[i][j]);
            }
            printf("\n");
        }
        printf("\n");
        for(i=0;i<5;i++)
        {
            for(j=0;j<5;j++)
            {
                printf("%4d",arr2[i][j]);
            }
            printf("\n");
        }
        printf("Please enter the player number:");
        scanf("%d",&player);
        while(1)
        {
            computer=rand()%25+1;
            if(check[25]==0)
                check[computer-1]=1;
                break;
        }
        bool out=true;
        for(i=0;i<5 && out;i++)
        {
            for(j=0;j<5 && out;j++)
            {
                if(player==arr1[i][j])
                {

                    arr1[i][j]=0;
                    out = false;
                    }
            }
        }
        out=true;
        for(i=0;i<5 && out;i++)
        {
            for(j=0;j<5 && out;j++)
            {
                if(player==arr2[i][j])
                {

                    arr2[i][j]=0;
                    out = false;
                    }
            }
        }
        out=true;
        for(i=0;i<5 && out;i++)
        {
            for(j=0;j<5 && out;j++)
            {
                if(computer==arr1[i][j])
                {
                    arr1[i][j]=0;
                    out=false;
                }
            }
        }
        out=true;
        for(i=0;i<5 && out;i++)
        {
            for(j=0;j<5 && out;j++)
            {
                if(computer==arr2[i][j])
                {
                    arr2[i][j]=0;
                    out=false;
                }
            }
        }
        for(int k=0; k<5; k++)
            if((!arr1[k][0] && !arr1[k][1] && !arr1[k][2] && !arr1[k][3] && !arr1[k][4]) || 0){a++;}
         for(int z=0; z<5; z++)
            if((!arr1[0][z] && !arr1[1][z] && !arr1[2][z] && !arr1[3][z] && !arr1[4][z]) || 0){a++;}
            for(int k=0; k<5; k++)
                if((!arr2[k][0] && !arr2[k][1] && !arr2[k][2] && !arr2[k][3] && !arr2[k][4]) || 0){b++;}
            for(int z=0; z<5; z++)
                if((!arr2[0][z] && !arr2[1][z] && !arr2[2][z] && !arr2[3][z] && !arr2[4][z]) || 0){b++;}
            if((!arr1[0][0] && !arr1[1][1] && !arr1[2][2] && !arr1[3][3] && !arr1[4][4]) || 0){a++;}
            if((!arr1[0][4] && !arr1[1][3] && !arr1[2][2] && !arr1[3][1] && !arr1[4][0]) || 0){a++;}
            if((!arr2[0][0] && !arr2[1][1] && !arr2[2][2] && !arr2[3][3] && !arr2[4][4]) || 0){b++;}
            if((!arr2[0][4] && !arr2[1][3] && !arr2[2][2] && !arr2[3][1] && !arr2[4][0]) || 0){b++;}
            if(a==5 && b == 5)
            {
                printf("tie\n");
                break;
            }
            else if(a == 5 && b != 5)
            {
                printf("Player win\n");
                break;
            }
            else if(a != 5 && b == 5)
            {
                printf("Computer win\n");
                break;
            }
    }
}
